class Create<%=class_name %>DesktopApplication
  def self.up
    DesktopApplication.create(
      :description => '<%=description %>',
      :icon => '<%=icon %>',
      :internal_identifier => '<%=file_name %>'
    )
  end

  def self.down
    DesktopApplication.destroy_all(['internal_identifier = ?','<%=file_name %>'])
  end
end
